{
  "name": "utr-template-v2",
  "author": "unshift (Takumi Hasegawa)",
  "version": "2.0.0",
  "description": "utr (unshift task runner) template. Please use Yarn.",
  "license": "MIT",
  "private": true,
  "config": {
    "utr": {
      "cli": "./dev/utr/utr-cli.js",
      "taskfile": "./dev/utr/task.js"
    }
  },
  "scripts": {
    "initProj": "node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile initProj",
    "clean": "node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile clean",
    "start": "cross-env NODE_ENV=development node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile start",
    "dev": "cross-env NODE_ENV=development node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile start",
    "build": "cross-env NODE_ENV=development node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile build",
    "build:prd": "cross-env NODE_ENV=production  node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile build",
    "dist": "cross-env NODE_ENV=production  node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile build",
    "watch": "cross-env NODE_ENV=development node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile watch",
    "watch:prd": "cross-env NODE_ENV=production  node $npm_package_config_utr_cli -t $npm_package_config_utr_taskfile watch"
  },
  "resolutions": {
    "pug-inheritance/pug-dependency/pug-lexer": "3.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/node": "^14.14.20",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-pug": "^7.0.1",
    "browser-sync": "^2.26.13",
    "chalk": "^4.1.0",
    "commander": "^6.2.1",
    "core-js": "^3.8.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.0.1",
    "gaze": "^1.1.3",
    "glob": "^7.1.6",
    "globule": "^1.3.2",
    "glslify-loader": "^2.0.0",
    "html-minifier": "^4.0.0",
    "json-loader": "^0.5.7",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "postcss-sort-media-queries": "^3.0.0",
    "proxy-middleware": "^0.15.0",
    "pug": "^3.0.0",
    "pug-inheritance": "^3.0.0-beta.2",
    "pug-plain-loader": "^1.1.0",
    "raw-loader": "^4.0.2",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.32.0",
    "sass-graph": "^3.0.5",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "webpack": "^5.11.1",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "devDependencies": {
    "@types/gsap": "^1.20.2",
    "dat.gui": "^0.7.7",
    "glsl-easings": "^1.0.0",
    "glsl-noise": "^0.0.0",
    "gsap": "^3.5.1",
    "mobile-detect": "^1.4.4",
    "three": "^0.124.0",
    "webfontloader": "^1.6.28"
  }
}
